	border 1px solid black
	width 50%

#pie
	background azulCima
	width 100%
	text-align center
#registro
	background azulCima
	border-radius .5em
	position relative
	padding 2em
	margin  auto
	text-align left
	ul
		list-style none

#slider
	border-color black
	position relative
	width 100%
	overflow hidden
	z-index 10
	ul
		list-style none
		position relative
		li
			float left

#right
	color white
	border-radius .5em
	position relative
	font-size 2em
	float right
	width 60px
	margin 10px
	padding .2em
	text-align center
	cursor pointer
	animation 2s right infinite
@keyframes right
	0%
		background white
		color black
	100%
		background rgb(65,182,230)

#left
	background rgb(65,182,230)
	color white
	border-radius .5em
	font-size 2em
	padding .2em
	position static
	text-align center
	float left
	width 60px
	margin 10px
	cursor pointer
	animation 2.8s right infinite
@keyframes right
	0%
		background white
		color black
	100%
		background rgb(65,182,230)
#numbers
	text-align center
	list-style none
	width 105px
	li
		position relative
		float left
		width 15px
		height 15px
		margin 0 50px
		border 1px solid #ccc

.banner
	border-radius 2em
	text-align center
	max-width 100%
	padding 0
	margin 0px
	position static
	img
		border-radius 0 5em 0
		box-shadow 7px 7px 5px 0px (rgba(50, 50, 50, 0.45))
		width 70%

.bannerP
	position absolute
	top 0px
	left -232px
	display block
	width 200px
	height 200px
.confianza

	margin auto
	width 90%
	img
		padding 0 0 0 30px
		width 22%

.container
	margin 10px auto
	overflow auto
	width 70%
	#navbar
		background-color black
		display block
		height 40px
	#navbar a
		color gris
		display inline-block
		height 40px
		line-height 37px
		padding 0 10px
	#navbar a.active, #navbar a:hover
		background-color #333
		border-radius .5em
		a
			text-decoration none
			outline none
			color gris
			display inline-block
			height 40px
			line-height 37px
			padding 0 10px
.contProductos
	background grey
	position relative
	width 70%
	height 100%
	z-index 50000
	float left
	display inline-block
	vertical-align top

.contSlider
	width 100%
	text-align center
	float right
	border-left 1px solid black
	border-top 1px solid black
	border-right 1px solid black
	border-bottom 1px solid black

.contenedorV
	overflow hidden
	padding-bottom 56.25%
	padding-top 30px
	position relative
	text-align center
	width 100%
	iframe, object, embed, video
		height 100%
		left 0
		position absolute
		top 0
		width 100%
	@media screen and (min-width 400px)
		width 100%
		@media screen and (min-width 600px)
			width 100%
			@media screen and (min-width 800px)
				width 100%
				@media screen and (min-width 1200px)
					width 100%

.coverflow
	perspective 2000
	perspective-origin 480px 300px
.coverflow ul
	margin 0
	padding 0
	width 50000px
	transform-style preserve-3d
	height 550px
.coverflow ul li
	margin-left -160px
	margin-right -160px
	text-align center
	transition all 0.5s ease-in-out
	z-index 1
	border 2px solid white
.coverflow ul li .left
	transform-rotate Y (45deg)
	transform-origin 0px
	transform scale(0.75, 0.75) translateY (-20px)
.coverflow ul li .right
	transform rotateY -45deg
	transform-origin 400px
	transform scale(0.75, 0.75) translateY (-20px)
.coverflow ul li .current
	background #FFF
	width 400px
	margin-left -100px
	margin-right -100px
	transform-origin 200px
	transform tranlateZ 100px
	z-index 1000
.pieInfo
	width 45%
