	width 99%
#main
	display block
	border-color black
	border thin
	overflow hidden
	margin 0 auto
	main-height 475px
	width 90%

	img
		width 80%
		margin auto
#main li
	float left
	height 96px
	list-style none outside none
	margin 6px
	position relative
	width 125px
#main ul
	overflow hidden
#main ul.hidden
	display none
#mapa
	background azulCima
	margin auto
	width 60%
	img
		width 100%
#marcas
	background rojo
	width 100%
#mm0
	background azulCima
	color white
	font-size 1.2em
