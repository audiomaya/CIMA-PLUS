@import 'nib/vendor'
@import 'nib/gradients'

gris = #DFDFDF
rojo = #FF0000
azul = #041787
primarioAzul1 = #1C2665
primarioAzul2 = #010D58
primarioAzul3 = #3549C3
primarioAzul4 = #5B69C3
secAzul1 = #01577C
secAzul2 = #18485D
secAzul3 = #003851
secAzul4 = #3193BD
secAzul5 = #569EBD
azulClaro = (rgba( 71, 166, 196, 0.65))
azulCima = linear-gradient(top, white, azul)

vendor-prefixes = o ms moz webkit

body
	background-color white
	font-size 18px
	font-family 'Oswald',sans-serif
	margin 0
	padding 0
	width 100%
button
	background-color rgb(0,193,213)
	color white
	border-radius 2em
	font-size 1.2em
	padding .5em
	a
		text-decoration none
		color white
label
	display block
h1
	font-weight 700
h2
	font-weight 300
p
	font-weight 300

#btnEnviar
	background-color azul
	color white
	border-radius .5em
	font-size 1.2em
	padding .5em
#contVideo
	border-radius .5em
	text-align center
	margin 10%
	padding auto
	width 85%
#contenedorLCis
	background-image url(../img/imgCisternas.jpg)
	background-repeat no-repeat
	background-position right
	background-color white
	text-align center
	height 700px
	max-width max-content
		@media screen and (min-width 375px)
			background url(../img/cimaBodegaG.jpg )no-repeat 90%

#contContact
	text-align center
	margin auto
	width 100%
	p
		font-weight 400
		letter-spacing: 0.1em;
		line-height 2
		text-align right
	h2
		font-weight 700
		color rojo
		text-align right

#foto
	display inline-block
	margin auto
	vertical-align top
	width 30%
	img
		width 70%
#info
	display inline-block
	margin auto
	vertical-align top
	width 40%
#imagen1
	background url(../img/cimaBodegaG.jpg) no-repeat 100%
	text-align center
	margin 10px
	width 100%
	@media screen and (min-width 375px)
			background url(../img/cimaBodegaG.jpg )no-repeat 90%
#imagen2
	background url(../img/cimaMundo.jpg) no-repeat 100%
	border-radius 0 2em 2em 0
	text-align center
	margin 10px
	width 100%
	@media screen and (min-width 375px)
			background url(../img/cimaMundo.jpg )no-repeat 90%
#imagen3
	border-radius 0 2em 2em 0
	text-align center
	margin 10px
	width 100%
#imagen4
	background url(../img/materialEle.jpg) no-repeat 100%
	border-radius 0 2em 2em 0
	text-align center
	margin 10px
	width 100%
